{
	"name": "wp-starter-theme",
	"version": "1.0.0",
	"description": "A custom WordPress starter theme.",
	"scripts": {
		"dev": "webpack --watch --progress --mode=development",
		"build": "webpack --config webpack.config.js --mode=production",
		"eslint": "eslint \"src/**/*.{js,jsx}\" --quiet --fix",
		"eslint:fix": "eslint \"src/**/*.{js,jsx}\" --quiet --fix",
		"format": "prettier --write \"src/**/*.{js,jsx,scss}\"",
		"stylelint": "stylelint \"src/**/*.{css,scss}\"",
		"stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
		"wp-pot": "wp-pot --src 'gcm.php' --dest-file 'languages/gcm.pot' --domain 'gcm'"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/davemoz/WP-Starter-Theme.git"
	},
	"author": "Dave Mozdzanowski",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/davemoz/WP-Starter-Theme/issues"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"homepage": "https://github.com/davemoz/WP-Starter-Theme#readme",
	"devDependencies": {
		"@babel/cli": "^7.12.10",
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@wordpress/eslint-plugin": "^8.0.1",
		"assert": "^2.0.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.2",
		"browser-sync": "^2.26.14",
		"browser-sync-webpack-plugin": "^2.3.0",
		"buffer": "^6.0.3",
		"constants-browserify": "^1.0.0",
		"css-loader": "^5.0.1",
		"css-minimizer-webpack-plugin": "^1.2.0",
		"eslint": "^7.19.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-webpack-plugin": "^2.4.3",
		"file-loader": "^6.2.0",
		"fs": "0.0.1-security",
		"imagemin": "^7.0.1",
		"mini-css-extract-plugin": "^1.3.6",
		"node-sass": "^5.0.0",
		"node-sass-glob-importer": "^5.3.2",
		"os-browserify": "^0.3.0",
		"path-browserify": "^1.0.1",
		"postcss": "^8.2.4",
		"postcss-loader": "^4.2.0",
		"postcss-preset-env": "^6.7.0",
		"prettier": "^2.2.1",
		"sass-loader": "^10.1.1",
		"stream-browserify": "^3.0.0",
		"style-loader": "^2.0.0",
		"stylelint": "^13.9.0",
		"stylelint-config-wordpress": "^17.0.0",
		"stylelint-order": "^4.1.0",
		"stylelint-scss": "^3.18.0",
		"stylelint-webpack-plugin": "^2.1.1",
		"typescript": "^4.1.3",
		"url-loader": "^4.1.1",
		"util": "^0.12.3",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0",
		"webpack-remove-empty-scripts": "^0.7.1",
		"webpack-watched-glob-entries-plugin": "^2.1.9",
		"wp-pot-cli": "^1.5.0"
	},
	"dependencies": {
		"@babel/polyfill": "^7.12.1",
		"aos": "^2.3.4"
	}
}
